{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/NavBar.jsx", "components/WalletContextProvider.jsx", "components/SendSolForm.js", "tailwind.config.js", "pages/api/hello.jsx", "pages/index.jsx", "pages/_app.jsx", "pages/_document.js"],
  "exclude": ["node_modules"]
}
